; Cypress specific USB Driver Installation INF for the Softkinetic DIVA 
; Processor support for x86 based platforms
; Operating system:  Windows 7, Windows 8, Windows 8.1
; (c) Copyright 2014 SoftKinetic Sensors NV.

[Version]
Signature="$WINDOWS NT$"
Class=Image
ClassGUID={6bdd1fc6-810f-11d0-bec7-08002be2092f}
provider=%SKDIVA_Provider%
CatalogFile=SoftKinetic_DIVA.cat
DriverVer=01/29/2015,1.0.0.002

[SourceDisksNames]
1=%SKDIVA_Install%,,,

[SourceDisksFiles]
SoftKinetic_DIVA.sys = 1

[DestinationDirs]
SKDIVA.Files.Ext = 10,System32\Drivers

[ControlFlags]
ExcludeFromSelect = *

[Manufacturer]
%SKDIVA_Provider%=Device,NT,NTx86,NTamd64

;for all platforms
[Device]
%VID_2113&PID_0002.DeviceDesc%=SKDIVA, USB\VID_2113&PID_0002
%VID_2113&PID_0003.DeviceDesc%=SKDIVA, USB\VID_2113&PID_0003


;for windows 2000 non intel platforms
[Device.NT]
%VID_2113&PID_0002.DeviceDesc%=SKDIVA, USB\VID_2113&PID_0002
%VID_2113&PID_0003.DeviceDesc%=SKDIVA, USB\VID_2113&PID_0003


;for x86 platforms
[Device.NTx86]
%VID_2113&PID_0002.DeviceDesc%=SKDIVA, USB\VID_2113&PID_0002
%VID_2113&PID_0003.DeviceDesc%=SKDIVA, USB\VID_2113&PID_0003


;for x64 platforms
[Device.NTamd64]
%VID_2113&PID_0002.DeviceDesc%=SKDIVA, USB\VID_2113&PID_0002
%VID_2113&PID_0003.DeviceDesc%=SKDIVA, USB\VID_2113&PID_0003


[SKDIVA]
CopyFiles=SKDIVA.Files.Ext
AddReg=SKDIVA.AddReg

[SKDIVA.HW]
AddReg=SKDIVA.AddReg.Guid

[SKDIVA.Services]
Addservice = SKDIVA,2,SKDIVA.AddService

[SKDIVA.NT]
CopyFiles=SKDIVA.Files.Ext
AddReg=SKDIVA.AddReg

[SKDIVA.NT.HW]
AddReg=SKDIVA.AddReg.Guid

[SKDIVA.NT.Services]
Addservice = SKDIVA,2,SKDIVA.AddService


[SKDIVA.NTx86]
CopyFiles=SKDIVA.Files.Ext
AddReg=SKDIVA.AddReg

[SKDIVA.NTx86.HW]
AddReg=SKDIVA.AddReg.Guid

[SKDIVA.NTx86.Services]
Addservice = SKDIVA,2,SKDIVA.AddService

[SKDIVA.NTamd64]
CopyFiles=SKDIVA.Files.Ext
AddReg=SKDIVA.AddReg

[SKDIVA.NTamd64.HW]
AddReg=SKDIVA.AddReg.Guid

[SKDIVA.NTamd64.Services]
Addservice = SKDIVA,2,SKDIVA.AddService


[SKDIVA.AddReg]
; Deprecating - do not use in new apps to identify a SoftKinetic DIVA driver
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,SoftKinetic_DIVA.sys
; You may optionally include a check for DriverBase in your application to check for a SoftKinetic DIVA driver
HKR,,DriverBase,,SoftKinetic_DIVA.sys
HKR,"Parameters","MaximumTransferSize",0x10001,4096
HKR,"Parameters","DebugLevel",0x10001,2
HKR,,FriendlyName,,%SKDIVA_Description%

[SKDIVA.AddService]
DisplayName    = %SKDIVA_Description%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %10%\System32\Drivers\SoftKinetic_DIVA.sys
AddReg         = SKDIVA.AddReg
LoadOrderGroup = Base

[SKDIVA.Files.Ext]
SoftKinetic_DIVA.sys

[SKDIVA.AddReg.Guid]
HKR,,DriverGUID,,%SKDIVA.GUID%

[Strings]
SKDIVA_Provider    = "SoftKinetic"
SKDIVA_Description = "SoftKinetic DIVA"
SKDIVA_Install     = "SoftKinetic DIVA Driver Installation Disk"
VID_2113&PID_0002.DeviceDesc="SoftKinetic DIVA"
VID_2113&PID_0003.DeviceDesc="SoftKinetic DIVA"
SKDIVA.GUID="{AE18AA60-7F6A-11d4-97DD-00010229B959}"
SKDIVA_Unused      = "."
